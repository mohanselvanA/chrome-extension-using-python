{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "A Chrome extension with JavaScript and HTML files.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/facebook.png",
      "48": "icons/instagram.png",
      "128": "icons/whatsapp.png"
    }
  }
  